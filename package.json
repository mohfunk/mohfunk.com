    "@rollup/plugin-url": "^4.0.0",
    "@rollup/plugin-yaml": "^2.0.0",
    "@semantic-release/changelog": "^3.0.6",
    "@semantic-release/git": "^7.0.18",
    "@types/lodash": "^4.14.149",
